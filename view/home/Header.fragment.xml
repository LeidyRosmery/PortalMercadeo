<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
  <Bar class="wrapper-bar heaHeader">
    <contentLeft>
      <Button icon="sap-icon://person-placeholder"  press="onPressProfile"/>
      <Image class="logo-home" src="img/logo.png"></Image>
    </contentLeft>
    <contentMiddle>
      <Title id="headerTitulo" class="headerTitulo" text="{utilModel>/tituloApp}"></Title>
    </contentMiddle>
    <contentRight>
      <Button icon="sap-icon://message-popup" text="{usuarioAlertas>/numAlertas}" press="onPressMessages"/>
    </contentRight>
  </Bar>
</core:FragmentDefinition>
